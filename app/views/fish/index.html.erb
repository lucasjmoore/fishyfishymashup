<div>
  <h1>Every Fish Caught By Everyone Ever</h1>
</div>
<div class="btn btn-success whiteText">
  <%= link_to "Caught a new fish!", new_fish_path %>
</div>

<% @fish.each do |fish| %>
  <div>
    <div>
    
      <!-- JS geolocation map all fish -->
    </div>
    <div><!-- intent to span 1-->
      Fish image
    </div>
    <div><!-- intent to span 7-->
      <strong><%= link_to fish.user.profile_name, user_profile_path(fish.user) %></strong> caught a
      <dl>
        <%= fish.fish_type %>
      <%#= fish.length %>
      <%#= fish.weight %>
      
        <div>
          <p><%= link_to time_ago_in_words(fish.created_at) + " ago", fish %>
            <span class="admin">
              <% if signed_in? && current_user == fish.user %>
                <%= link_to 'Show', fish %>
                <%= link_to 'Edit', edit_fish_path(fish) %>
                <%= link_to 'Delete', fish, method: :delete, data: { confirm: 'Are you sure?' } %>
              <% end %>
            </span>
          </div>
      </div>
  </div>
<% end %>
